    {
      "id": "exfil.system_prompt.reveal_system_prompt",
      "category": "prompt_exfiltration",
      "patternType": "regex",
      "pattern": "(system\\s+prompt|reveal\\s+\\S+\\s+system)",
      "flags": "i",
      "risk": "high",
      "score": 0.8,
      "tags": [
        "prompt_exfil",
        "system_disclosure"
      ],
      "summary": "System prompt disclosure attempt detected."
    },
    {
      "id": "exfil.print_instructions",
      "category": "prompt_exfiltration",
      "patternType": "regex",
      "pattern": "(print|show|display|output)\\s+(your|the)\\s+(system\\s+)?(prompt|instructions|rules)",
      "flags": "i",
      "risk": "high",
      "score": 0.8,
      "tags": [
        "prompt_exfil",
        "instructions"
      ],
      "summary": "Request to print or show model instructions detected."
    },
